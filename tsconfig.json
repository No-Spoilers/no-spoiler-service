{
  "include": [
    "."
  ],
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2022",
    "module": "nodenext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "checkJs": false,
    "rootDir": ".",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
  },
  "exclude": [
    ".github",
    ".vscode",
    "bruno",
    "lambda",
    "node_modules",
    "test",
    "coverage"
  ]
}